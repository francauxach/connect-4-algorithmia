  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "c8": "^7.11.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7",
